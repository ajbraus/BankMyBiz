<tr>
  <td><strong><%= post.title %></strong></td>
  <td class="muted"><%= post.desc %></td>
  <td><%= raw post.tags.map(&:name).map { |t| link_to "#" + t, tag_path(t), class:"label" }.join(' ') %></td>
  <td><%= link_to "view", post_path(post) %> <%= render :partial => "commitments/commit_reneg", locals: { post: post } %></td>
</tr>