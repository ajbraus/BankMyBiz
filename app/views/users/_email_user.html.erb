<%= link_to user_url(other_user), class:"no-hover" do %>
  <table>
    <tr>
      <td><img src="<%= image_url(other_user) %>" width="80" style='margin-right:10px; border-radius:100px;'></td>
      <td>
        <strong><%= other_user.first_name_with_last_initial %></strong>, <span class="text-muted"><%= other_user.try(:hq_state) %></span> <br>
          <%= other_user.position %> at <%= other_user.org_name %><br>
          <%= current_user.percentage_match(other_user) %>% Match<br>
          <%= other_user.status %>
      </td>
    </tr>
  </table>
<% end %>
